import discord
from discord.ext import commands

intents = discord.Intents.default()
intents.members = True          # Necesario para gestionar roles.
intents.message_content = True  # Necesario si vas a usar comandos de texto (como '!enviarrangos').


bot = commands.Bot(command_prefix='/', intents=intents)


@bot.event
async def on_ready():
    print('Bot Connectedâœ… With Minis APIðŸ”—')
    print(f'ID LOGGED: {bot.user.id}')
    print('MI DISCORD SERVER https://discord.gg/2MPJ68rj')
    print(f'CONSOLE FOR BOT: {bot.user.name}')
#token here
TOKEN = 'TOKEN HERE' 

if __name__ == "__main__":
    bot.run(TOKEN)
